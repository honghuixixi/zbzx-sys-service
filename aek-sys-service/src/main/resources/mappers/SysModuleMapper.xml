<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.sys.mapper.SysModuleMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.sys.model.SysModule">
		<id column="id" property="id" />
		<result column="name" property="name" />
		<result column="version_number" property="versionNumber" />
		<result column="module_type" property="moduleType" />
		<result column="price_policy_id" property="pricePolicyId" />
		<result column="url" property="url" />
		<result column="create_by" property="createBy" />
		<result column="create_time" property="createTime" />
		<result column="update_by" property="updateBy" />
		<result column="update_time" property="updateTime" />
		<result column="enable" property="enable" />
		<result column="del_flag" property="delFlag" />
		<result column="custom_data" property="customData" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		id, name, version_number AS versionNumber, module_type AS moduleType,
		price_policy_id AS pricePolicyId, url, create_by AS createBy, create_time
		AS createTime, update_by AS updateBy, update_time AS updateTime,
		enable, del_flag AS delFlag, custom_data AS customData
	</sql>
</mapper>
