<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.sys.mapper.SysLoginLogMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.sys.model.SysLoginLog">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="name" property="name" />
		<result column="ip" property="ip" />
		<result column="login_time" property="loginTime" />
		<result column="client_type" property="clientType" />
		<result column="browser_type" property="browserType" />
		<result column="browser_version" property="browserVersion" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, name, ip, login_time AS loginTime, client_type AS clientType, browser_type AS browserType, browser_version AS browserVersion
    </sql>
</mapper>
